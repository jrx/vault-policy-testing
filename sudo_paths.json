{
    "sudo_paths": [
        { "path": "auth/token/accessors$",
          "actions": ["list"]
        },
        { "path": "auth/token/create$",
          "actions": ["create","update"]
        },
        {
            "path": "auth/token/revoke-orphan$",
            "actions": ["create","update"]
        },
        {
            "path": "pki/root/sign-self-issued$",
            "actions": ["create","update"]
        },
        {
            "path": "pki/root$",
            "actions": ["delete"]
        },
        {
            "path": "sys/audit$",
            "actions": ["read"]
        },
        {
            "path": "sys/audit/.*",
            "actions": ["create","update","delete"]
        },
        {
            "path": "sys/auth/.*",
            "actions": ["read","create","update","delete"]
        },
        {
            "path": "sys/auth/.*/tune$",
            "actions": ["read","create","update"]
        },
        {
            "path": "sys/config/auditing/request-headers$",
            "actions": ["read"]
        },
        {
            "path": "sys/config/auditing/request-headers/.*",
            "actions": ["read","create","update","delete"]
        },
        {
            "path": "sys/config/cors$",
            "actions": ["read","create","update","delete"]
        },
        {
            "path": "sys/config/ui/headers$",
            "actions": ["read","list"]
        },
        {
            "path": "sys/config/ui/headers/.*",
            "actions": ["create","update","delete"]
        },
        {
            "path": "sys/internal/inspect/router/.*",
            "actions": ["read"]
        },
        {
            "path": "sys/leases/lookup/.*",
            "actions": ["list"]
        },
        {
            "path": "sys/leases/revoke-force/.*",
            "actions": ["create","update"]
        },
        {
            "path": "sys/leases/revoke-prefix/.*",
            "actions": ["create","update"]
        },
        {
            "path": "sys/plugins/catalog/.*",
            "actions": ["read","create","update","delete"]
        },
        {
            "path": "sys/raw/.*",
            "actions": ["read","create","update","delete","list"]
        },
        {
            "path": "sys/remount$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/replication/reindex$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/replication/performance/primary/secondary-token$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/replication/dr/primary/secondary-token$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/rotate$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/seal$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/step-down$",
            "actions": ["create","update"]
        },
        {
            "path": "sys/storage/raft/snapshot-auto/config$",
            "actions": ["list"]
        },
        {
            "path": "sys/storage/raft/snapshot-auto/config/.*",
            "actions": ["read","create","update","delete"]
        }
    ],
    "sudo_prefixes": [
        "sys",
        "pki",
        "auth"
    ]
}